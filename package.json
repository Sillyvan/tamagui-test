{
  "name": "nutriswiss",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "generate-themes": "tamagui generate-themes ./theme-builder.ts ./theme-output.ts"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/native": "^6.1.17",
    "@tamagui/font-inter": "1.102.1",
    "@tamagui/lucide-icons": "^1.102.1",
    "@tamagui/react-native-media-driver": "1.102.1",
    "@tamagui/shorthands": "1.102.1",
    "@tamagui/themes": "1.102.1",
    "expo": "^51.0.18",
    "expo-camera": "~15.0.13",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.17",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.3.1",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.17.1",
    "react-native-reanimated": "~3.13.0",
    "react-native-safe-area-context": "4.10.8",
    "react-native-screens": "3.32.0",
    "react-native-web": "~0.19.12",
    "tamagui": "1.102.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@tamagui/babel-plugin": "1.102.1",
    "@types/react": "~18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^9.6.0",
    "eslint-config-universe": "^13.0.0",
    "prettier": "^3.3.2",
    "typescript": "~5.5.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
